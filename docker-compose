version: '2'
services:
  jenkins:
    image: jenkinsci/blueocean:latest
    container_name: jenkins
    user: jenkins
    volumes:
    - /data/jenkins:/var/jenkins_home
    - /var/run/docker.sock:/var/run/docker.sock
    environment:
      JENKINS_HOST_HOME: "/data/jenkins"
    ports:
    - "9001:8080"
